<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Количество слов"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="64dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Время раунда"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="134dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Штраф за пропуск"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="209dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Звук в игре"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="302dp" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="304dp"
            tools:layout_editor_absoluteY="302dp" />

        <Switch
            android:id="@+id/switch2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="290dp"
            tools:layout_editor_absoluteY="209dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="для достижения победы"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="83dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="продолжительность в секундах"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="153dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="каждое пропущенное слово отнимает одно очко"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="236dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="включить звуковые эффекты"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="321dp" />

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="345dp"
            android:layout_height="18dp"
            android:max="120"
            android:progress="60"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="102dp" />

        <SeekBar
            android:id="@+id/seekBar2"
            android:layout_width="345dp"
            android:layout_height="15dp"
            android:max="120"
            android:progress="60"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="172dp" />

        <TextView
            android:id="@+id/textView9"
